<% form_for(@college_benefit) do |f| %>
  <fieldset>
    <legend>添加学院补贴</legend>
    <%= f.error_messages %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <p class="bbar">
      <%= f.submit '保存' %>
      <input type="button" onclick="$page.showListView()" value="取消" />
    </p>
  </fieldset>
<% end %>