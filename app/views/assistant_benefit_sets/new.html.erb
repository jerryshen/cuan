<% form_for :assistant_benefit_set, :url => { :action => 'create'} do |f| %>
  <fieldset>
    <legend>设置辅导员津贴</legend>
    <%= render :partial => 'form', :locals => {:f =>f}  %>
    <p class="bbar">
      <input type="button" value='保存并新增' id ="save_and_new"  />
      <%= f.submit '保存并返回' %>
      <input type="button" onclick="$page.showListView()" value="返回列表" />
    </p>
  </fieldset>
<% end %>
