<% form_for(@fee_cutting_record) do |f| %>
  <fieldset>
    <legend>添加扣款记录</legend>
    <%= f.error_messages %>
    <%= render :partial => 'form', :locals => { :f => f }  %>
    <p class="bbar">
      <%= f.submit '保存' %>
      <input type="button" onclick="$page.showListView()" value="取消" />
    </p>
  </fieldset>
<% end %>